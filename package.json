{
  "name": "flucord-boilerplate",
  "description": "A simple boilerplate for the Flucord framework - a framework for building Discord bots",
  "author": {
    "name": "Aldin Čamdžić",
    "email": "acamdzic0@proton.me"
  },
  "license": "CC BY-NC 4.0",
  "scripts": {
    "start": "bun run src/index.ts",
    "dev": "bun run --watch src/index.ts",
    "biome": "biome check --write --unsafe",
    "prisma-case-format": "prisma-case-format --file=prisma/schema.prisma --map-field-case=snake --map-table-case=snake,plural"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "1.2.2",
    "prisma": "^6.3.0",
    "prisma-case-format": "^2.2.1"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@prisma/client": "6.3.1",
    "@sapphire/ratelimits": "^2.4.11",
    "discord.js": "^14.17.3",
    "flucord": "1.4.8"
  }
}
